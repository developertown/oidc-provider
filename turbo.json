{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"],
      "cache": true
    },
    "e2e": {
      "outputs": ["test-results/**"],
      "dependsOn": [],
      "cache": true
    },
    "test": {
      "outputs": [],
      "dependsOn": [],
      "cache": true
    },
    "test:coverage": {
      "outputs": ["coverage/**"],
      "dependsOn": [],
      "cache": true
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "preview": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "format:check": {
      "cache": true
    },
    "format:fix": {
      "cache": true
    },
    "license:check": {
      "cache": true
    }
  }
}
